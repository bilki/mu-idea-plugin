<idea-plugin>
    <id>io.higherkindness.mu-idea</id>
    <name>Mu IDEA</name>
    <version>0.0.1</version>
    <vendor url="https://github.com/higherkindness/mu-idea-plugin">Higherkindness</vendor>

    <description>IntelliJ IDEA plugin for Mu - https://github.com/higherkindness/mu</description>

    <idea-version since-build="191.0"/>

    <depends>org.intellij.scala</depends>

    <extensions defaultExtensionNs="org.intellij.scala">
        <syntheticMemberInjector implementation="higherkindness.mu.MuInjector"/>
    </extensions>
</idea-plugin>
